all: build-rgbgame build-uarttest build-libgame build-keydemo

clean:
	make -C rgbgame clean
	make -C uarttest clean
	make -C libgame clean
	make -C keydemo clean

build-uarttest:
	make -C uarttest

build-rgbgame:
	make -C rgbgame

build-libgame:
	make -C libgame

build-keydemo:
	make -C keydemo
